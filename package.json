{
  "name": "blogilista_backend",
  "version": "1.0.0",
  "private": true,
  "description": "backend blogilistalle",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npx eslint . --fix",
    "start": "cross-env NODE_ENV=production node index.js",
    "start:test": "cross-env NODE_ENV=test node index.js",
    "dev": "cross-env NODE_ENV=development node --watch index.js",
    "test": "cross-env NODE_ENV=test node --test --test-concurrency=1",
    "build:ui": "rm -rf dist && cd ../frontend && npm run build && cp -r dist ../backend",
    "deploy:full": "cd ../.. && git subtree push --prefix Osa5/backend https://github.com/Markus-Kivinen/blogilista.git main"
  },
  "author": "Markus Kivinen",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^8.14.3",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@stylistic/eslint-plugin-js": "^4.2.0",
    "eslint": "^9.26.0",
    "globals": "^16.1.0",
    "supertest": "^7.1.1"
  }
}
